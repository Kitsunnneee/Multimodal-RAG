# Storage Configuration
# Set to 'true' to use Google Cloud Storage, 'false' to use local storage
USE_GCS=false

# Google Cloud Configuration (only needed if USE_GCS=true)
# GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json
# GOOGLE_CLOUD_PROJECT=your-project-id
# GOOGLE_CLOUD_LOCATION=us-central1
# GCS_BUCKET=your-bucket-name
# VECTOR_SEARCH_INDEX_NAME=mm_rag_langchain_index
# VECTOR_SEARCH_ENDPOINT_ID=mm_rag_langchain_index_endpoint

# Application Settings
LOG_LEVEL=INFO
CACHE_DIR=./.cache

# Model Configuration (optional)
# MODEL_NAME=gemini-2.0-flash
# EMBEDDING_MODEL_NAME=text-embedding-005
# MULTIMODAL_EMBEDDING_MODEL=multimodalembedding

# Vertex AI Configuration (for image embeddings)
# VERTEX_AI_PROJECT=your-project-id
# VERTEX_AI_LOCATION=us-central1
# VERTEX_AI_ENDPOINT=projects/your-project-id/locations/us-central1/publishers/google/models/multimodalembedding


# Optional: Proxy settings if needed
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=http://proxy.example.com:8080
